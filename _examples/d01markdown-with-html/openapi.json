{
  "components": {
    "schemas": {
      "Error": {
        "description": "Error is custom error response",
        "properties": {
          "message": {
            "type": "string"
          }
        },
        "required": [
          "message"
        ],
        "title": "Error",
        "type": "object"
      }
    }
  },
  "info": {
    "description": "This is the example has text/html output",
    "title": "hello",
    "version": "1.0.0"
  },
  "openapi": "3.0.0",
  "paths": {
    "/api/hello": {
      "post": {
        "operationId": "main.Hello",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "properties": {
                  "name": {
                    "type": "string"
                  }
                },
                "required": [
                  "name"
                ],
                "type": "object"
              }
            }
          }
        },
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "properties": {
                    "message": {
                      "type": "string"
                    }
                  },
                  "required": [
                    "message"
                  ],
                  "type": "object"
                }
              }
            },
            "description": ""
          },
          "default": {
            "description": ""
          }
        },
        "tags": [
          "main"
        ]
      }
    },
    "/hello/{name}": {
      "get": {
        "operationId": "main.HelloHTML",
        "parameters": [
          {
            "in": "path",
            "name": "name",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "content": {
              "text/html": {
                "schema": {
                  "type": "string"
                }
              }
            },
            "description": "html with greeting message"
          },
          "default": {
            "description": ""
          }
        },
        "tags": [
          "main"
        ]
      }
    },
    "/hello2/{name}": {
      "get": {
        "operationId": "main.HelloHTML2",
        "parameters": [
          {
            "in": "path",
            "name": "name",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "content": {
              "text/html": {
                "schema": {
                  "type": "string"
                }
              }
            },
            "description": "html with greeting message"
          },
          "default": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Error"
                }
              }
            },
            "description": "default error response"
          }
        },
        "tags": [
          "main"
        ]
      }
    }
  },
  "servers": [
    {
      "description": "local development server",
      "url": "http://localhost:8888"
    }
  ]

{
  "components": {
    "schemas": {
      "SortOrder": {
        "enum": [
          "desc",
          "asc"
        ],
        "type": "string",
        "x-new-type": true
      },
      "Time": {
        "format": "date-time",
        "type": "string"
      },
      "Todo": {
        "properties": {
          "createdAt": {
            "$ref": "#/components/schemas/Time"
          },
          "done": {
            "type": "boolean"
          },
          "id": {
            "type": "string"
          },
          "title": {
            "type": "string"
          }
        },
        "type": "object"
      }
    }
  },
  "info": {
    "description": "-",
    "title": "Sample API",
    "version": "0.0.0"
  },
  "openapi": "3.0.0",
  "paths": {
    "/todo": {
      "get": {
        "operationId": "main.ListTodo",
        "parameters": [
          {
            "in": "query",
            "name": "sort",
            "schema": {
              "$ref": "#/components/schemas/SortOrder"
            }
          }
        ],
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "items": {
                    "$ref": "#/components/schemas/Todo"
                  },
                  "type": "array"
                }
              }
            },
            "description": ""
          },
          "default": {
            "description": ""
          }
        }
      }
    },
    "/todo/{id}": {
      "get": {
        "operationId": "main.GetTodo",
        "parameters": [
          {
            "in": "path",
            "name": "id",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Todo"
                }
              }
            },
            "description": ""
          },
          "default": {
            "description": ""
          }
        }
      }
    }
  },
  "servers": [
    {
      "url": "http://localhost:8888",
      "description": "local development server"
    }
  ]
}
